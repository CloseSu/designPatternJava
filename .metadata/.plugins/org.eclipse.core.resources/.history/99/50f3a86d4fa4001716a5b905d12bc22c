package com.example.demo.observer;

public class StockObserver {
	private String name;
	private Subject sub;
	
	public StockObserver(String name, Subject sub) {
		this.name = name;
		this.sub = sub;
	}
	
	public void closeStockMarket() {
		System.out.println(sub.getSubjectState() + " " + name +  "關閉股票行情，繼續工作!!");
	}
}
